{
  "compilerOptions": {
    "lib": ["es2017"],
    "baseUrl": "./src/",
    "removeComments": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es2017",
    "outDir": "lib",
    "strictNullChecks": true,
    "esModuleInterop": true
  }
}
